
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <!-- basic -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- mobile metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1">
    <!-- site metas -->
    <title>shree</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- fevicon -->
    <link rel="icon" href="images/fevicon.png" type="image/gif" />
    <!-- bootstrap css -->
    <link href="~/Content/TrangChuCSS/bootstrap.min.css" rel="stylesheet" />
    <!-- style css -->
    <link href="~/Content/TrangChuCSS/style.css" rel="stylesheet" />
    <link href="~/Content/WebCSS.css" rel="stylesheet" />
    <!-- Responsive-->
    <link href="~/Content/TrangChuCSS/responsive.css" rel="stylesheet" />
    <!-- Scrollbar Custom CSS -->
    <script src="~/Scripts/TrangChuJS/jquery.mCustomScrollbar.concat.min.js"></script>
    <!-- Tweaks for older IEs-->
    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.css" media="screen">
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script><![endif]-->
    @Styles.Render("~/Content/TrangChuCSS/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<!-- body -->

<body class="main-layout">
    <!-- header -->
    <header>
        <!-- header inner -->
        <div class="header-top">
            <div class="header">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-2 col-lg-4 col-md-4 col-sm-3 col logo_section">
                            <div class="full">
                                <div class="center-desk">
                                    <div class="logo">
                                        <a href="index.html"><img src="~/Icons/logo.png" alt="#" /></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-10 col-lg-8 col-md-8 col-sm-9">

                            <div class="menu-area">
                                <div class="limit-box">
                                    <nav class="main-menu ">
                                        <ul class="menu-area-main">
                                            <li class="active"> @Html.ActionLink("TRANG CHỦ", "Index", "Store") </li>
                                            <li> @Html.ActionLink("LIÊN HỆ", "Gioithieu", "Store")</li>
                                            <li> @Html.ActionLink("SẢN PHẨM", "Sanpham", "MainStore")</li>
                                            <li> @Html.ActionLink("TIN TỨC", "IndexTinTuc", "Store")</li>
                                            <li>@Html.Action("GioHangPartial", "GioHang")</li>
                                            <li class="nav-item">
                                                <div class="dropdown">
                                                    <button class="dropbtn" style="font-size:16px"><b>NGƯỜI DÙNG</b></button>
                                                    <div class="dropdown-content">
                                                        <a href="@Url.Action("Dangnhap","KhachHang")">ĐĂNG NHẬP</a>
                                                        <a href="@Url.Action("Dangky","KhachHang")">ĐĂNG KÝ</a>
                                                        <a href="@Url.Action("DoiPassword","KhachHang")">ĐỔI PASSWORD</a>
                                                    </div>
                                                </div>
                                            </li>
                                            <li> <a href="#"><img src="~/Icons/icon_b.png" alt="#" /></a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- end header inner -->
        <!-- end header -->
        <section class="slider_section">
            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                </ol>
                <div class="carousel-inner">
                    <div class="carousel-item active">

                        <div class="container-fluid padding_dd">
                            <div class="carousel-caption">

                                <div class="row">
                                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                                        <div class="text-bg">
                                            <span>Welcome To Shree</span>
                                            <h1>Shop Thực Phẩm</h1>
                                            <p>Chào mừng bạn đã đến với website bán thực phẩm sạch Shree của chúng tôi. Tại đây, bạn có thể tìm kiếm, đặt mua các loại sản phẩm được sản xuất và chế biến với công nghệ thân thiện với môi trường, đảm bảo an toàn thực phẩm. </p>
                                            <form class="Vegetable">
                                                <input class="Vegetable_fom" placeholder="" type="text" name=" Vegetable">
                                                <button class="Search_btn">Tìm Kiếm</button>
                                            </form>
                                            <a href="@Url.Action("Gioithieu","Store")">Liên Hệ</a> <a href="@Url.Action("Sptheoloai","MainStore", new { id = "2"})">Rau-Củ-Quả</a> <a href="@Url.Action("Sptheoloai","MainStore", new { id = "6"})">Thịt</a>
                                        </div>
                                    </div>

                                    <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                                        <div class="images_box">
                                            <figure><img src="~/Icons/img2.png"></figure>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        </section>
        </div>
    </header>



    <!-- about  -->
    <div id="about" class="about">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                    <div class="about-box">
                        <h2>GIỚI THIỆU</h2>
                        <p>Website bán thực phẩm sạch Shree được thành lập vào đầu năm 2022 với mục tiêu là giúp phân phối các loại thực phẩm, nông sản của các Hợp tác xã. Nhưng qua thời gian, với sự phát triển về công nghệ thông tin cùng với sự cạnh tranh trong việc phân phối sản phẩm. Website đã chuyển đổi cách thực hoạt động không chỉ phân phối giúp các Hợp tác xã mà còn hợp tác với các doanh nghiệp để kinh doanh sản phẩm của họ. Thông qua đó, website vừa có thể giúp các Hợp tác xã vừa có thể kinh doanh các sản phẩm khác để kiếm thêm kinh phí để website được hoạt động thông suốt.</p>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 padding_rl">
                    <div class="about-box_img">
                        <figure><img src="~/Icons/about.jpg" alt="#" /></figure>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="about" class="about">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                    <div class="about-box_img">
                        <figure><img src="~/Icons/about.jpg" alt="#" /></figure>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 padding_rl">
                    <div class="about-box">
                        <h2>MỤC TIÊU</h2>
                        <p>Mục tiêu hàng đầu của chúng tôi khi thành lập nên website này với các lý do sau:<br />
                        (1) Giúp các Hợp tác xã<br />
                        (2) Kinh doanh sản phẩm của các Doanh nghiệp<br />
                        (3) Cung cấp các loại nông sạch, đảm bảo an toàn thực phẩm<br />
                        (4) Cho người dùng hiểu được tầm quan trọng của việc sử dụng thực phẩm sạch<br />
                        Ngoài những lý do đã nêu ra bên trên, chúng tôi hoạt động trên tinh thần giúp đỡ các Hợp tác xã gặp khó khăn, cùng với đó là tôn chỉ "Khách hàng là trên hết" khi phục vụ các khách hàng đã sử dụng dịch vụ của website!</p>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="about" class="about">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12">
                    <div class="about-box">
                        <h2>THÀNH VIÊN NHÓM</h2>
                        <p>Gồm có 3 thành viên:<br />
                            <h3>Lê Quang Duy</h3><br />
                            <h3>Thái Bình Nhưỡng</h3><br />
                            <h3>Trần Thị Mai Tiên</h3>
                        </p>
                    </div>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 padding_rl">
                    <div class="about-box_img">
                        <figure><img src="~/Icons/about.jpg" alt="#" /></figure>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end abouts -->

    <br />

    <!-- end clients -->
    <!--  footer -->
    <footr>
        <div class="footer ">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <a href="#" class="logo_footer"> <img src="~/Icons/logo2.png" alt="#" /></a>
                    </div>
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 ">
                        <div class="row">
                            <div class="col-xl-4 col-lg-4 col-md-6 col-sm-6 ">
                                <div class="address">
                                    <h3>Địa Chỉ </h3>
                                    <ul class="loca">
                                        <li>
                                            <a href="#"></a>140/11 Bình Quới, P.27
                                            <br>Q.Bình Thanh, Tp.HCM
                                        </li>
                                        <li>
                                            <a href="#"></a>(+84 911977681)
                                        </li>
                                        <li>
                                            <a href="#"></a>nhuongthai8@gmail.com
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-lg-4 col-md-6 col-sm-6">
                                <div class="address">
                                    <h3>Mạng Xã Hội</h3>
                                    <ul class="Menu_footer">
                                        <li class="active"> <a href="#">Twitter</a> </li>
                                        <li><a href="#">Facebook</a> </li>
                                        <li><a href="#">Instagram</a> </li>
                                        <li><a href="#">Youtube</a> </li>
                                    </ul>
                                </div>
                            </div>

                            @Html.Action("NewsLetter", "Store")

                        </div>
                    </div>

                </div>

            </div>
            <div class="copyright">
                <div class="container">

                </div>
            </div>
        </div>
    </footr>
    <!-- end footer -->
    <!-- Javascript files-->
    <script src="~/Scripts/TrangChuJS/jquery.min.js"></script>
    <script src="~/Scripts/TrangChuJS/popper.min.js"></script>
    <script src="~/Scripts/TrangChuJS/bootstrap.bundle.min.js"></script>
    <script src="~/Scripts/TrangChuJS/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/TrangChuJS/plugin.js"></script>
    <!-- sidebar -->
    <script src="~/Scripts/TrangChuJS/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Scripts/TrangChuJS/custom.js"></script>
    <script src="https:cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.js"></script>


</body>

</html>


